<openerp>
<data>

<record id="hr_service_config_setting_view_form" model="ir.ui.view">
    <field name="name">hr_service.config_setting form</field>
    <field name="model">hr_service.config_setting</field>
    <field name="inherit_id" ref="hr_service_app.hr_service_config_setting_view_form"/>
    <field name="mode">extension</field>
    <field name="arch" type="xml">
        <xpath expr="//group[@name='feature_group']" position="after">
            <separator string="Recruitment"/>
            <group name="recruitment_config">
                <label for="id" string="General"/>
                <div>
                    <div>
                        <label for="id" string="Sequence" class="oe_inline"/>
                        <field name="recruitment_request_sequence_id" class="on_inline"/>
                    </div>
                </div>
            </group>
        </xpath>
    </field>
</record>

</data>
</openerp>
